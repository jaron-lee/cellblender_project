@article{Ma:JNeurophysiol:2015,
	author = {Ma, Jun  and Kelly, Lauren  and Ingram, Justin  and Price, Thomas J.  and Meriney, Stephen D.  and Dittrich, Markus},
	title = {New insights into short-term synaptic facilitation at the frog neuromuscular junction.},
	journal = {J Neurophysiol},
	uuid = {ACEE2DF2-E50A-4B35-A6A7-77BF1FBEE17F},
	volume = {113},
	number = {1},
